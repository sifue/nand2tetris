// File name: projects/01/Not.hdl
 /**
 * Not gate:
 * out = ((in == 0), 1, 0)
 */
CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // aとbに同じ値を入れたらNotの役割を果たしそう
    Nand(a=in, b=in, out=out);
    
}