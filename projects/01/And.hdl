/**
 * And gate:
 * out = (((a == 1) && (b == 1))), 1, 0) 
 */
CHIP And {
    IN a, b;
    OUT out;
    
    PARTS:
    // 我々の手持ちにある回路は、Nand (Not And) と Not だけである。
    Nand(a=a, b=b, out=nand);
    Not(in=nand, out=out);
}